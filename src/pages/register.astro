---
import Layout from "../layouts/Layout.astro"
import MyForm from "../components/sub-pages/FormSignin.astro"
---

<Layout title="Register" />
<Fragment>
  <MyForm 
    inputs = {[
      {
        type: 'email',
        name: 'email',
        label: 'Email',
      },
      {
        type: 'password',
        name: 'password',
        label: 'Password',
      },
    ]}    
    title = 'Register'
    text = 'Register a new account'
    action = '/api/v1/auth/register'
  />
</Fragment>