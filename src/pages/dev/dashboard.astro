---
import Layout from '~/layouts/Layout.astro'
import SectionContainer from '~/components/dev/MySectionComponent.astro'
import CallToAction from '~/components/main/CallToAction.astro'
import MainHeader from '~/components/dev/MainHeader.astro'
import MainHero from '~/components/dev/MainHero.astro'
import MainSearch from '~/components/dev/HeroSearch.astro'
import NewSection from '~/components/dev/NewSection.astro'

import { combineResponsive, RESPONSIVE, SPACING, COMPONENTS, LAYOUT } from '~/config/styleCostants'

let username = 'Doublefacessss'

---

<Layout title="Test Component">
  <SectionContainer
    id="section-container"
  > 
    <MainSearch 
      id="main-hero"  
      slot="MainSearch"
    />  
    <MainHero
      id="main-hero"
      slot="MainHero"
      title=""
      subititle="subtitle"
      user_name={username}
      tagline={`${username}'s Dashboard`}
    />
    <MainHeader 
      slot="MainHeader"
      id="main-header"
    />
    <div slot="CTA">
      <CallToAction 
        id="call-to-action"        
      />
    </div>
    <NewSection slot="MainCards" />
  </SectionContainer>
</Layout>
